generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String   @id @default(uuid())
  email           String?  @unique
  phoneNumber     String?  @unique
  isPhoneVerified Boolean  @default(false)
  name            String?
  picture         String?
  refreshToken    String?
  googleId        String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
